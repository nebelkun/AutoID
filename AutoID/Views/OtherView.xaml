<UserControl x:Class="AutoID.Views.OtherView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:viewModels="clr-namespace:AutoID.ViewModels">
	<UserControl.DataContext>
		<viewModels:OtherViewModel />
	</UserControl.DataContext>
	<StackPanel Orientation="Vertical" Margin="20">
		<Button Margin="10" Padding="10 5" Command="{Binding GenerateFakeDataCommand}" HorizontalAlignment="Center">Сгенерировать тестовые данные</Button>
		<Button Margin="10" Padding="10 5" Command="{Binding ConfigureEmailCommand}" HorizontalAlignment="Center">Сконфигурировать почту</Button>
	</StackPanel>
</UserControl>
